{
    "name": "dependency-checker",
    "description": "Analyses a pull request for dependency changes",
    "private": true,
    "license": "MIT",
    "author": "Hive HR <engineering@hive.hr>",
    "main": "index.ts",
    "engines": {
        "node": "12"
    },
    "scripts": {
        "start": "yarn pnpify ts-node index.ts",
        "build": "yarn pnpify tsc -p .",
        "lint": "yarn build --noEmit"
    },
    "dependencies": {
        "@actions/core": "1.1.3",
        "@actions/github": "1.1.0",
        "@types/node": "12.7.12",
        "@yarnpkg/pnpify": "2.0.0-rc.7",
        "execa": "^3.2.0",
        "ts-node": "8.4.1",
        "typescript": "3.6.4"
    },
    "installConfig": {
        "pnp": true
    }
}
